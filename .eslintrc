{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
    "airbnb", 
    "prettier"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": 0,
    "react/jsx-boolean-value": 0,
    "prettier/prettier": ["error"]
  }
}
